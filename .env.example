# Copy this file to ".env" and fill in your secrets.

# Database (Postgres)
# Example: postgresql://user:password@localhost:5432/invoice_db
DATABASE_URL="postgresql://invoice_user:secretpass@localhost:5432/invoice"
POSTGRES_USER=invoice_user
POSTGRES_PASSWORD=secretpass

# Note: install psycopg for DB access
# pip install "psycopg[binary]"

# Marker LLM configuration
MARKER_LLM_SERVICE=marker.services.openai.OpenAIService
MARKER_OPENAI_MODEL=gpt-5-mini
OPENAI_API_KEY=sk-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
OPENAI_BASE_URL=https://api.openai.com/v1

# LangSmith configuration
LANGSMITH_TRACING="true"
LANGSMITH_ENDPOINT="https://api.smith.langchain.com"
LANGSMITH_API_KEY="<your-api-key>"
LANGSMITH_PROJECT="pr-upbeat-eyeball-3"

# Structured Output configuration
STRUCTURED_OUTPUT_MODEL="gpt-5-mini"